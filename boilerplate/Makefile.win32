
SUBMAKEFILE = 1

include ../Makefile.win32

CFLAGS += -I../src

SOURCES = \
	cairo-boilerplate.c \
	cairo-boilerplate-win32.c \
	cairo-boilerplate-ps.c \
	cairo-boilerplate-svg.c \
	cairo-boilerplate-pdf.c \
	xmalloc.c \
	$(NULL)

OBJECTS = $(subst .c,.obj,$(SOURCES))

all: boiler.lib

boiler.lib: $(OBJECTS)
	lib -NOLOGO -OUT:$@ $(OBJECTS)

